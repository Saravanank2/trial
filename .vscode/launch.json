{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run trial-trialmodule",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/nstrialmodule/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/trialmodule",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/trial/trialmodule\"}"
      },
      "envFile": "${workspaceFolder}/trialmodule/.env1",
      "preLaunchTask": "Build trial"
    }
  ]
}